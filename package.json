{
  "name": "@dragonish/compre-face-sdk",
  "type": "module",
  "private": false,
  "version": "0.1.3",
  "description": "Exadel CompreFace Rest API TypeScript SDK",
  "author": "Light",
  "license": "MIT",
  "homepage": "https://github.com/LightAPIs/compre-face-sdk",
  "repository": "LightAPIs/compre-face-sdk",
  "main": "./index.js",
  "exports": {
    "import": "./index.js",
    "require": "./index.umd.cjs"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "tsx ./build.ts",
    "test": "tsx ./check.ts && cross-env NODE_NO_WARNINGS=1 mocha",
    "postinstall": "tsx ./env.ts"
  },
  "engines": {
    "node": ">=16.17.0 <17.0.0 || >=18.0.0"
  },
  "keywords": [
    "exadel",
    "CompreFace",
    "compre-face",
    "SDK",
    "Face Recognition"
